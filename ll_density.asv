function logDens = ll_density(param,getCov,getMean,data)
[d,n] = size(data);
sigma = getCov(param,d);
mu = getMean(param,d);
L = chol(Sigma, 'lower');
logDetSigma = 2 * sum(log(diag(L)));
Xc = data - mu';
Y = L \ Xc;
quadForms = sum(Y.^2, 1);   % 1Ã—n
log2pi = log(2*pi);
logDens = -0.5 * (n * (d*log(2*pi) + logDetSigma) + sum(quadForms));
%logDens = -n * ((0.5*d)*log(2*pi)+(0.5)*log(det(sigma)));
%for i=1:n
%    logDens = logDens -0.5*((data(:,i)-mu')'/sigma*(data(:,i)-mu'));
%end
end